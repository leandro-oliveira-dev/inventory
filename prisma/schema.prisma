
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Stock{
  id          String   @id @default(cuid())
  clienteId   String
  position    String
  productoId  String
  quantity    Int
  status      String
  
}

model  Client{
  id         String   @id @default(cuid())
  name       String
}

model user{
  id         String   @id @default(cuid())
  email      String   @unique
  password   String
  name       String
  role       String
}